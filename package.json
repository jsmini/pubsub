{
  "name": "@jsmini/pubsub",
  "version": "0.2.2",
  "description": "小而美的pubsub库，发布订阅者模式",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint -c .eslintrc.js 'src/**/*.js'",
    "build:self": "rollup -c config/rollup.config.js",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build:aio": "rollup -c config/rollup.config.aio.js",
    "build": "npm run clean && npm run build:self && npm run build:esm && npm run build:aio && npm run build:aio.min",
    "test": "npm run lint && cross-env NODE_ENV=test nyc mocha",
    "release": "npm test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags",
    "lint:fix": "eslint --fix -c .eslintrc.js 'src/**/*.js' --fix",
    "build:aio.min": "cross-env NODE_ENV=production npm run build:aio",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "yanhaijing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/jsmini/pubsub.git"
  },
  "bugs": {
    "url": "https://github.com/jsmini/pubsub/issues"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "cdkit": "1.1.0",
    "es5-shim": "4.5.10",
    "eslint": "^5.15.1",
    "expect.js": "0.3.1",
    "mocha": "3.5.3",
    "rimraf": "2.6.2",
    "rollup": "0.57.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-node-resolve": "3.0.3",
    "@js-lib/cli": "^1.3.0",
    "cross-env": "5.2.0",
    "@babel/cli": "7.1.5",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "rollup-plugin-uglify": "1.0.0",
    "@babel/register": "7.0.0",
    "babel-plugin-istanbul": "5.1.0",
    "coveralls": "3.0.2",
    "nyc": "13.1.0",
    "source-map-support": "0.5.9"
  },
  "dependencies": {
    "@jsmini/event": "^0.6.2",
    "@jsmini/is": "^0.8.4",
    "@babel/runtime": "^7.1.2"
  },
  "files": [
    "/dist"
  ],
  "engines": {
    "node": ">= 0.12.0"
  }
}
